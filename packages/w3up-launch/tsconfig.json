{
  "compilerOptions": {
    "composite": true,
    "outDir": "./dist",
    "allowJs": true,
    "checkJs": true,
    "target": "esnext",
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "strict": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "esModuleInterop": true,
    "noEmit": false,
    "declaration": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": false,
    "baseUrl": "./",
    "incremental": true,
    "jsx": "react"
  },
  "include": [
    "./src",
    "./test"
  ],
  "references": [
  ]
}
