  "floater_animations": [
    {
      "trigger": "pricing_hero_header",
      "duration": 1000,
      "offset": 700,
      "floaters": [
        {
          "id": "pricing_hero-coil",
          "default": "rotate(17deg)",
          "start": {
            "x": 1,
            "y": 20
          },
          "end": {
            "x": 10,
            "y": -200
          }
        },
        {
          "id": "pricing_hero-corkscrew",
          "start": {
            "rotate": -1,
            "scale": 1,
            "x": -1,
            "y": 1
          },
          "end": {
            "rotate": -25,
            "scale": 1.3,
            "x": -60,
            "y": 30
          }
        },
        {
          "id": "pricing_hero-cross",
          "start": {
            "scale": 1,
            "rotate": -1,
            "x": -1,
            "y": 1
          },
          "end": {
            "scale": 1.3,
            "rotate": -30,
            "x": -30,
            "y": 20
          }
        },
        {
          "id": "section_tiers-triangle",
          "default": "scaleX(-1)",
          "start": {
            "rotate": -100,
            "x": -1,
            "y": 1
          },
          "end": {
            "rotate": -145,
            "x": -80,
            "y": 60
          }
        }
